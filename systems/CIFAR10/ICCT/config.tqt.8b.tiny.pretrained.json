{
    "description": "TQT 8-Bit Version of Tiny-ICCT (1-4-64-64)",
    "data": {
        "train": {
            "dataset": {
                "transform": {
                    "class": "TransformB",
                    "kwargs": {
                        "augment": true
                    }
                }
            },
            "dataloader": {
                "bs": 512
            }
        },
        "valid": {
            "dataset": {
                "transform": {
                    "class": "TransformB",
                    "kwargs": {
                        "augment": false
                    }
                }
            },
            "dataloader": {
                "bs": 512
            }
        }
    },
    "network": {
        "class": "ICCT",
        "kwargs": {
            "num_layers": 1,
            "num_heads": 4,
            "embedding_dim": 64,
            "projection_dim": 64,
            "pool": "mean",
            "pretrained": "/usr/scratch2/sassauna1/wiesep/quantlab/systems/CIFAR10/ICCT/logs/exp0000/fold0/saves/best.ckpt"
        },
        "quantize": {
            "function": "pact_recipe",
            "kwargs": {
                "config": {
                    "Softmax": {
                        "kwargs": {
                            "mode": "ITA-Partial"
                        }
                    },
                    "PACTConv2d": {
                        "tokenizer.conv_layers.0.0$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_channel",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTLinear": {
                        ".*mhsa.WQ$": {},
                        ".*mhsa.WK$": {},
                        ".*mhsa.WV$": {},
                        ".*mhsa.out$": {},
                        "transformer.layers.*.*.fn.ff.0$": {},
                        "transformer.layers.*.*.fn.ff.3$": {},
                        "classifier.0$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_layer",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTUnsignedAct": {
                        "tokenizer.conv_layers.0.2": {
                            "act_kind": "relu"
                        },
                        "classifier.1$": {
                            "act_kind": "relu"
                        },
                        "kwargs": {
                            "n_levels": 256,
                            "init_clip": "max",
                            "nb_std": 3,
                            "tqt": true,
                            "learn_clip": true,
                            "rounding": true,
                            "noisy": false,
                            "upper_percentile": 1.0,
                            "lower_percentile": 0.0,
                            "leaky": 0.0
                        }
                    },
                    "harmonize": {
                        "n_levels": 256,
                        "init_clip": "max",
                        "noisy": false,
                        "rounding": true,
                        "force_out_eps": true,
                        "tqt": true,
                        "learn_clip": true
                    }
                }
            }
        }
    },
    "training": {
        "loss_fn": {
            "class": "CrossEntropyLoss",
            "kwargs": {}
        },
        "gd": {
            "opt": {
                "class": "AdamW",
                "kwargs": {
                    "lr": 1e-5
                },
                "takes_net": false
            },
            "lr_sched": {
                "class": "ReduceLROnPlateau",
                "kwargs": {}
            }
        },
        "n_epochs": 3,
        "quantize": {
            "function": "get_pact_controllers",
            "kwargs": {
                "schedules": {
                    "linear": {
                        "1": [
                            "verbose_on",
                            "start"
                        ],
                        "2": "freeze"
                    },
                    "activation": {
                        "1": [
                            "verbose_on",
                            "start"
                        ],
                        "2": "freeze"
                    },
                    "eps": {
                        "1": "verbose_on",
                        "2": "start"
                    }
                },
                "kwargs_linear": {
                    "update_every": "batch"
                },
                "kwargs_activation": {
                    "init_clip_hi": 6.0
                }
            }
        }
    },
    "meters": {
        "train": {
            "compute_profiling_statistic": true,
            "compute_task_statistic": true,
            "tensor_statistics": [
                {
                    "class": "CIFAR10InputsSnapshot",
                    "kwargs": {
                        "name": "tokenizer.conv_layers.0.0",
                        "start": 0,
                        "period": 98,
                        "n_inputs": 4,
                        "preprocessing_type": 1
                    }
                },
                {
                    "class": "OutputFeaturesSnapshot",
                    "kwargs": {
                        "name": "tokenizer.conv_layers.0.0",
                        "start": 0,
                        "period": 98,
                        "n_inputs": 4
                    }
                }
            ]
        },
        "valid": {
            "compute_profiling_statistic": true
        }
    },
    "experiment": {
        "target_loss": "valid",
        "ckpt_period": 1
    }
}